
public class Linear_Search {
	
	public static void main(String[] args) {

        int arr[] = {0,4,7,9,14};
        int target=9;

        for(int i=0;i<arr.length;i++){
            if(arr[i]==target){
                System.out.println("Element is present at index: "+i);
            }
            else System.out.println("Element not prsenet");
        }
